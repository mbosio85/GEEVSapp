<div class= "SearchPanel">
	<h3></h3>
       <table align="center">	
       	<tr>
       	   <td>
       	   <%= form_tag(:controller => 'gapp',:action => 'search_chr_pos_show') do %>
           Chromosome: <%= select_tag "chromosome", options_for_select(@chr,@searchPanelChr), :prompt => "Select" %> &nbsp;
           Start: <%= text_field_tag :start_pos, @searchPanelStartPos, :class => 'sfieldOptional' %> &nbsp;
           End: <%= text_field_tag :end_pos, @searchPanelEndPos, :class => 'sfieldOptional' %> &nbsp;
           Platform: <%= select_tag "chPlatform", "<option selected=\"selected\">Illumina</option><option>Solid</option>".html_safe %> &nbsp;
           Gene definition: <%= select_tag "chGeneDef", "<option selected=\"selected\">Refseq</option><option>Ensembl</option><option>UCSC/Known</option>".html_safe %>  &nbsp;
           Exonic Function: <%= select_tag "chFunction", "<option selected=\"selected\">All</option><option>Exonic</option><option>Splicing</option><option>Intronic</option><option>UTR3</option><option>UTR5</option><option>Downstream</option><option>Upstream</option><option>Intergenic</option><option>ncRNA_exoic</option><option>ncRNA_splicing</option><option>ncRNA_intronic</option><option>ncRNA_UTR3</option><option>ncRNA_UTR5</option>".html_safe %>  &nbsp;
           <br>
           Variant Function: <%= select_tag "varfunction","<option selected=\"selected\">All</option><option>Nonsynonymous</option><option>Synonymous</option><option>Stopgain</option><option>Stoploss</option>".html_safe %> &nbsp;
           Allele Frequency: <%= text_field_tag :af ,@searchPanelAF, :class => 'sfieldPanel' %>&nbsp;
       	   </td>
       	   <td>
       	   <%= submit_tag("Search/Filter") %>
       	   <% end %>
       	   </td>
       	</tr>	
       </table>
</div>

<h2 align="center">Showing Results for <%= "chr"+@searchPanelChr+":"+@searchPanelStartPos+"-"+@searchPanelEndPos %></h2>
<div class="search_res">
	<table id="variants">
		<thead>
		<tr>
			<th><span title="Chromosome">Chr</span></th>
			<th><span title="Position (1-based coordinate)">Position</span></th>
			<th><span title="Reference allele">Reference</span></th>
			<th><span title="Alteration/Observed allele">Alteration</span></th>
			<th><span title="Allele frequency in GEEVS">Allele Freq</span></th>
			<th><span title="Allele count in GEEVS">Allele Count</span></th>
			<th><span title="Allele number in GEEVS">Allele Number</span></th>
			<th><span title="The gene hit by the variant">Gene</span></th>
			<th><span title="The transcript hit by the variant">Transcript</span></th>
			<th><span title="Amino acid change">AAC</span></th>
			<th><span title="Coding region impact of the variant">Variant Fucntion</span></th>
			<th><span title="Transcript region hit by the variant">Exonic Function</span></th>
			<th><span title="PhyloP conservation score of placental mammal">PhyloP(Pm)</span></th>
			<th><span title="PhyloP conservation score of primates">PhyloP(Pr)</span></th>
			<th><span title="PhyloP conservation score of vertebrates">PhyloP(vt)</span></th>
			<th><span title="PhastCons conservation score of placental mammal">PhastCons(Pm)<span></th>
			<th><span title="PhastCons conservation score of primates">PhastCons(Pr)</span></th>
			<th><span title="PhastCons conservation score of vertebrates">PhastCons(vt)</span></th>
			<th><span title="GERP++ conservation score 1">GERP++(s1)</span></th>
			<th><span title="GERP++ conservation score 2">GERP++(s2)</span></th>
			<th><span title="SIFT score of the variant">SIFT</span></th>
			<th><span title="Polyphen2 score of the variant">Pph2</span></th>
			<th><span title="Mutationassessor score of the variant">MutAss</span></th>
			<th><span title="Condel score of the variant">Condel</span></th>
			<th><span title="dbSNP rs identifier">dbSNP</span></th>
			<th><span title="Global minor allele frequency of the dbSNP rs identifier">dbSNP gmaf</span></th>
			<th><span title="Allele frequency of the European dataset in Washington Exome Variant Server">AF(Eur:EVS)</span></th>
			<th><span title="Allele frequency of the African dataset in Washington Exome Variant Server">AF(Afr:EVS)</span></th>
			<th><span title="Allele frequency of the Total dataset in Washington Exome Variant Server">AF(Tot:EVS)</th>
			<th><span title="Allele frequency of the European dataset in 1000Genomes">AF(Eur:1000G)</span></th>
			<th><span title="Allele frequency of the African dataset in 1000Genomes">AF(Afr:1000G)</span></th>
			<th><span title="Allele frequency of the American dataset in 1000Genomes">AF(Amr:1000G)</span></th>
			<th><span title="Allele frequency of the Asian dataset in 1000Genomes">AF(Asia:1000G)</span></th>
			<th><span title="Allele frequency of the Total dataset in 1000Genomes">AF(Tot:1000G)</span></th>
			<th><span title="Segmental duplication score">SegDup</span></th>
		</tr>
		</thead>
		<tbody>
		<% @res.each do |r1,r2,r3,r4,r5,r6,r7,r8,r9,r10,r11,r12,r13,r14,r15,r16,r17,r18,r19,r20,r21,r22,r23,r24,r25,r26,r27,r28,r29,r30,r31,r32,r33,r34,r35,r36,r37,r38,r39| -%>
		<tr>
			<td><%= r2 %></td> 
			<td><%= r3 %></td>
			<td><%= r4 %></td>
			<td><%= r5 %></td>
			<td><%= r31 %></td>
			<td><%= r32 %></td>
			<td><%= r33 %></td>
			<td><%= r35 %></td>
			<td><%= r36 %></td>
			<td><%= r37 %></td>
			<td><%= r38 %></td>
			<td><%= r39 %></td>
			<td><%= r8 %></td>
			<td><%= r9 %></td>
			<td><%= r10 %></td>
			<td><%= r11 %></td>
			<td><%= r12 %></td>
			<td><%= r13 %></td>
			<td><%= r14 %></td>
			<td><%= r15 %></td>
			<td><%= r16 %></td>
			<td><%= r17 %></td>
			<td><%= r18 %></td>
			<td><%= r19 %></td>
			<td><%= r20 %></td>
			<td><%= r21 %></td>
			<td><%= r22 %></td>
			<td><%= r23 %></td>
			<td><%= r24 %></td>
			<td><%= r25 %></td>
			<td><%= r26 %></td>
			<td><%= r27 %></td>
			<td><%= r28 %></td>
			<td><%= r29 %></td>
			<td><%= r30 %></td>						
		</tr>
		<% end -%>
		</tbody>
	</table>
</div>